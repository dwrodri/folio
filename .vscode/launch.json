    "version": "0.2.0",
    "configurations": [
        {
            "name": "clang (Mac) - build and debug SimpleVec tests",
            "type": "cppdbg",
            "request": "launch",
            "targetArchitecture": "arm64",
            "program": "${workspaceFolder}/build/tests/simplevec_test",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "MIDebuggerPath": "/usr/bin/gdb",
        },
        {
            "name": "clang (Debian) - build and debug SimpleVec tests",
            "type": "cppdbg",
            "request": "launch",
            "targetArchitecture": "x64",
            "program": "${workspaceFolder}/build/tests/simplevec_test",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "MIDebuggerPath": "/usr/bin/gdb",
        },
        {
            "name": "clang (Debian) - build and debug SimpleMat tests",
            "type": "cppdbg",
